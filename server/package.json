  },
  "dependencies": {
    "@elysiajs/cookie": "^0.8.0",
    "@elysiajs/cors": "1.2.0",
    "@elysiajs/jwt": "^1.2.0",
    "@types/bun": "^1.2.14",
    "@types/nodemailer": "^6.4.17",
    "@types/qrcode": "^1.5.5",
    "argon2": "^0.41.1",
    "crypto": "^1.0.1",
    "date-fns": "^4.1.0",
    "decimal.js": "^10.5.0",
    "dotenv": "^16.4.7",
    "drizzle-orm": "^0.43.1",
    "elysia": "1.2.25",
    "logsnag": "^1.0.0",
    "lru-cache": "^11.0.2",
    "nodemailer": "^6.10.0",
